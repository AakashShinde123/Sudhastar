{

    "version":2,
    "builds":[
        {
            "src": "foodzone/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "50mb","runtime": "Python 3.13.0"}

        },
        {
          
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": {
             "distDir": "staticfiles_build"
          }        }  

    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "foodzone/wsgi.py"
        }
    ]
}