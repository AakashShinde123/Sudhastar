{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/python"
    }
  ],
  "outputDirectory": "staticfiles_build",
  "env": {
    "DJANGO_SETTINGS_MODULE": "SUDHASTAR.settings",
    "PYTHON_VERSION": "3.x"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/SUDHASTAR/$1"
    }
  ]
}
